<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_bon_consum_form" model="ir.ui.view">
            <field name="name">bon.consum.form</field>
            <field name="model">bon.consum</field>
            <field name="arch" type="xml">
                <form string="Register Payment">
                    <group>
                        <field name="data_from"/>
                        <field name="date_to"/>

                    </group>
                    <footer>
                        <button string="Crear reporte" name="action_print_report" type="object" class="oe_highlight" data-hotkey="q"/>
                        <button string="Cancelar" class="btn btn-secondary" special="cancel" data-hotkey="z"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
